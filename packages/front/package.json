{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "relay-compiler --validate && tsc && vite build",
    "preview": "vite preview",
    "relay": "relay-compiler"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-relay": "14.1.3",
    "@types/relay-runtime": "14.1.6",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-plugin-relay": "^14.1.0",
    "graphql": "^16.6.0",
    "relay-compiler": "^14.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-relay": "^2.0.0"
  },
  "relay": {
    "src": "./src",
    "language": "typescript",
    "schema": "./src/schema.graphql",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "eagerEsModules": true
  }
}